<Window x:Class="MeContrata.JsonShow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MeContrata"
        mc:Ignorable="d"
        Title="JsonShow" Height="450" Width="450" Background="Black">
    <Grid>
        <TextBox Text="Dados em Json do usuário cadastrado" HorizontalAlignment="Left" TextWrapping="Wrap"  VerticalAlignment="Top" Width="322" Height="414" Background="Black" Foreground="White" FontSize="18" Margin="70,0,0,0" SelectionBrush="{x:Null}" BorderBrush="{x:Null}" />
        <TextBox  x:Name="JsonContent" HorizontalAlignment="Center" TextWrapping="Wrap"  VerticalAlignment="Center" Width="430" Height="358" Background="Black" Foreground="White" />


    </Grid>
</Window>
